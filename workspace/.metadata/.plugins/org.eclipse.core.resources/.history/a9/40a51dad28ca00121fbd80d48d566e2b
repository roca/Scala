package main.scala.rosalind


import scala.io.Source._

object Wabbits {
  def main(args: Array[String]): Unit = {

  val dna_seq = fromFile(args(0), "utf-8").getLines.mkString
    println(dnaCount(dna_seq).mkString(" "))
  }

  def fibonacciNumbersUpTo(n: Int): List[Int] = {

  	val fibs: Stream[Int] = 0 #:: 1 #:: ((fibs zip fibs.tail) map { case (c1,c2) => c1 + c2 })

  	  fibs.take(n).toLost

  }

}