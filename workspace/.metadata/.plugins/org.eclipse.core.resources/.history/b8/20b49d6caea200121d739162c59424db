package week4

object nth {
  def nth[T](n: Int, xs: List[T]): T =
   if (n == 0) xs.head
   else nth(n-1, xs.tail)                         //> nth: [T](n: Int, xs: week4.List[T])T
   
   
   val list = new Cons(1, new Cons(2, new Cons(3, new Nil)))
                                                  //> list  : week4.Cons[Int] = week4.Cons@80d3d6f
   
   nth(0,list)                                    //> res0: Int = 1
}