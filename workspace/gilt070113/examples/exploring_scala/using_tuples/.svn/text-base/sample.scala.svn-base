val person = ("John", "Smith", 25)
println(person)

println(person._1)
println(person._2)
println(person._3)

//println(person._5) //compilation error
//for stepping out of bounds

val (firstName, lastName, age) = person
println(firstName)
println(lastName)
println(age)

val two = (1, 2)
println(two)

val reversedTwo = two.swap
println(reversedTwo)







